<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_height="@dimen/create_account_complete_dialog_height"
    style="@style/LoginCreateAccountDialogLayoutStyle.Success">

    <TextView
        android:id="@+id/title"
        android:text="@string/acct_created_title"
        style="@style/DialogTitleStyle"/>

    <View
        android:id="@+id/titleAccent"
        style="@style/DialogAccentStyle"
        android:layout_marginBottom="31dp"/>

    <TextView
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:textSize="14dp"
        android:text="@string/acct_created_message"
        style="@style/LoginCreateAccountTextView" />

    <TextView
        android:id="@+id/emailTextView"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginTop="15dp"
        android:layout_marginBottom="15dp"
        style="@style/EmailTextViewStyle" />

    <com.heb.dtn.widget.DTNRoundedButton
        android:id="@+id/okButton"
        style="@style/OKButton" />

</LinearLayout>